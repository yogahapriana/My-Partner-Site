<% if current_user.is_admin %>
  <div id="form_universal">
    <%= form_tag do %>
      <p id="notice"><%= notice %></p>
      <p style="font-size: 15px">
        <b>Name:</b>
        <%= @group.name %>
      </p>
      <%= link_to 'Edit', edit_group_path(@group) %> |
      <%= link_to 'Back', groups_path %>
    <% end %>
  </div>
<% end %>